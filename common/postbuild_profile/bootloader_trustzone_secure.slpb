parameters:
  - name: build_dir
constants:
  - name: project_name
    value: bootloader-secure
steps:
  - task: copy
    input: "{{build_dir}}/{{project_name}}.s37"
    output: "../artifact/{{project_name}}.s37"
    export: bootloader_secure_app
  - task: "copy"
    output: "artifact/trustzone_secure_library.o"
    input: "{{build_dir}}/trustzone_secure_library.o"